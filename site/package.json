  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-spring/web": "^9.4.1",
    "@types/lodash": "^4.14.186",
    "@vercel/commerce": "workspace:*",
    "@vercel/commerce-bigcommerce": "workspace:*",
    "@vercel/commerce-commercejs": "workspace:*",
    "@vercel/commerce-kibocommerce": "workspace:*",
    "@vercel/commerce-local": "workspace:*",
    "@vercel/commerce-ordercloud": "workspace:*",
    "@vercel/commerce-saleor": "workspace:*",
    "@vercel/commerce-sfcc": "workspace:*",
    "@vercel/commerce-shopify": "workspace:*",
    "@vercel/commerce-spree": "workspace:*",
    "@vercel/commerce-swell": "workspace:*",
    "@vercel/commerce-vendure": "workspace:*",
    "@yomo/presencejs": "1.0.0-alpha.4",
    "autoprefixer": "^10.4.2",
    "axios": "^1.1.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^1.1.1",
    "email-validator": "^2.0.4",
    "ioredis": "^5.2.3",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.7.0",
    "lodash": "^4.17.21",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "nanoid": "^4.0.0",
    "next": "^12.3.0",
    "next-themes": "^0.2.0",
    "postcss": "^8.3.5",
    "postcss-nesting": "^10.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.1",
    "react-hot-toast": "^2.4.0",
    "react-merge-refs": "^2.0.1",
    "react-use-measure": "^2.1.1",
    "swr": "^1.3.0",
    "tabbable": "^5.2.1",
    "tailwindcss": "^3.0.13",
    "@vercel/analytics": "^0.1.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@tailwindcss/forms": "^0.5.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^13.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}